{
  "workbench.productIconTheme": "fluent-icons",
  "workbench.startupEditor": "newUntitledFile",
  "keyboard.dispatch": "keyCode",
  "editor.fontFamily": "'Fira code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.selectionHighlight": false,
  "editor.occurrencesHighlight": false,
  "editor.minimap.enabled": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.colorCustomizations": {
    "editorError.foreground": "#00000000",
    "editorWarning.foreground": "#00000000",
    "editorInfo.foreground": "#00000000",
    "editor.snippetTabstopHighlightBackground": "#00000000",

  },
  "python.languageServer": "Pylance",
  "editor.suggestSelection": "first",
  "files.trimTrailingWhitespace": true,
  "workbench.editor.restoreViewState": false,
  "files.restoreUndoStack": false,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "notebook.globalToolbar": false,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.folding": false,
  "explorer.confirmDelete": false,
  "editor.formatOnPaste": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "terminal.external.linuxExec": "/usr/bin/kitty",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.fontSize": 13,
  "security.workspace.trust.untrustedFiles": "open",
  "github.gitAuthentication": false,
  "settingsSync.keybindingsPerPlatform": false,
  "editor.lightbulb.enabled": false,
  "git.autofetch": true,
  "workbench.list.automaticKeyboardNavigation": false,
  "editor.guides.highlightActiveIndentation": false,
  "editor.scrollBeyondLastLine": false,
  "workbench.colorTheme": "One Dark Pro",
  "window.menuBarVisibility": "hidden",
  "window.titleBarStyle": "custom",
  "vsintellicode.features.python.deepLearning": "enabled",
  "terminal.explorerKind": "external",
  "notebook.cellToolbarLocation": {
    "default": "hidden"
  },
  "notebook.navigation.allowNavigateToSurroundingCells": false,
  "breadcrumbs.enabled": false,
  "files.exclude": {
    "**/__pycache__": true,
    "**/.ipynb_checkpoints": true,
    "**/.python-version": true,
    "**/.vscode": true
},
  "notebook.consolidatedRunButton": true,
  "extensions.ignoreRecommendations": true,
  "editor.semanticHighlighting.enabled": false,
  "vsintellicode.typescript.completionsEnabled": false,
  "jupyter.sendSelectionToInteractiveWindow": true,
  "notebook.diff.ignoreOutputs": true,
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "/home/rnd/.vimrc",
  "vim.normalModeKeyBindings": [
        {
            "before": ["leader", "w"],
            "commands": ["workbench.action.files.save"]
        },
        {
            "before": ["<leader>", "f", "f"],
            "after": [],
            "commands": [{
                    "command": "workbench.action.quickOpen",
                    "args": []
                }]
        },
        {
            "before": ["<leader>", "f", "n"],
            "after": [],
            "commands": [{
                    "command": "workbench.action.openSettingsJson",
                    "args": []
                }]
        },
    ],
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "notebook.markup.fontSize": 14,
    "files.autoSave": "onFocusChange",
    "workbench.iconTheme": "material-icon-theme",
    "telemetry.telemetryLevel": "off",
    "editor.parameterHints.enabled": false,
    "python.analysis.typeCheckingMode": "basic",
    "terminal.integrated.enablePersistentSessions": false,
    "jupyter.askForKernelRestart": false,
    "notebook.output.textLineLimit": 25,
    "window.zoomLevel": 0.6,
    "oneDarkPro.italic": false,
    "scm.diffDecorations": "none",
    "explorer.decorations.colors": false,
    "workbench.editor.decorations.badges": false,
    "workbench.editor.decorations.colors": false,
}

