{
  "oneDarkPro.italic": false,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.startupEditor": "newUntitledFile",
  "keyboard.dispatch": "keyCode",
  "editor.fontFamily": "'Fira code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.selectionHighlight": false,
  "editor.occurrencesHighlight": false,
  "editor.minimap.enabled": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.colorCustomizations": {
    "editorError.foreground": "#00000000",
    "editorWarning.foreground": "#00000000",
    "editorInfo.foreground": "#00000000"
  },
  "python.languageServer": "Pylance",
  "editor.suggestSelection": "first",
  "files.trimTrailingWhitespace": true,
  "workbench.editor.restoreViewState": false,
  "files.restoreUndoStack": false,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "notebook.globalToolbar": false,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.folding": false,
  "explorer.confirmDelete": false,
  "editor.formatOnPaste": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "terminal.external.linuxExec": "/usr/bin/kitty",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.fontSize": 13,
  "security.workspace.trust.untrustedFiles": "open",
  "github.gitAuthentication": false,
  "settingsSync.keybindingsPerPlatform": false,
  "editor.lightbulb.enabled": false,
  "git.autofetch": true,
  "notebook.output.textLineLimit": 50,
  // "vscode-neovim.neovimExecutablePaths.linux": "/usr/local/bin/nvim",
  "workbench.list.automaticKeyboardNavigation": false,
  "editor.guides.highlightActiveIndentation": false,
  "window.zoomLevel": 0.6,
  "editor.scrollBeyondLastLine": false,
  "workbench.colorTheme": "One Dark Pro",
  "window.menuBarVisibility": "hidden",
  "window.titleBarStyle": "custom",
  "vsintellicode.features.python.deepLearning": "enabled",
  "editor.parameterHints.enabled": false,
  "terminal.explorerKind": "external",
  "notebook.cellToolbarLocation": {
    "default": "hidden"
  },
  "notebook.navigation.allowNavigateToSurroundingCells": false,
  "python.pythonPath": "/usr/bin/python3",
  "breadcrumbs.enabled": false,
  "files.exclude": {
    "**/.ipynb_checkpoints": true,
    "**/.python-version": true,
    "**/.vscode": true
  },
  "notebook.consolidatedRunButton": true,
  "editor.suggestOnTriggerCharacters": false,
  "extensions.ignoreRecommendations": true,
  "editor.semanticHighlighting.enabled": false,
  "vsintellicode.typescript.completionsEnabled": false,
  "jupyter.sendSelectionToInteractiveWindow": true,
  "workbench.iconTheme": "vscode-icons",
  "notebook.diff.ignoreOutputs": true,
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "vim.normalModeKeyBindings": [
        {
            "before": ["<Tab>"],
            "after": ["%"]
        },
        {
            "before": ["leader", "w"],
            "commands": ["workbench.action.files.save"]            ]
        },
        {
            "before": ["<Leader>", "v"],
            "after": ["<C-v>"]
        },
        {
            "before": ["H"],
            "after": ["0"]
        },
        {
            "before": ["L"],
            "after": ["$"]
        },
        {
            "before": ["J"],
            "after": ["<C-d>"]
        },
        {
            "before": ["K"],
            "after": ["<C-u>"]
        },
        {
            "before": ["<Leader>", "<Leader>", "p"],
            "after": ["o", "<Esc>", "p"]
        },
        {
            "before": ["<C-u>"],
            "after": ["v", "i", "w", "U", "<Esc>"]
        },
    ],
  "vim.visualModeKeyBindings": [
        {
            "before": ["<Tab>"],
            "after": ["%"]
        },
        {
            "before": ["H"],
            "after": ["0"]
        },
        {
            "before": ["L"],
            "after": ["$"]
        },
        {
            "before": ["<Leader>", "p"],
            "after": ["\"", "_", "d", "P"]
        },
        {
            "before": ["<"],
            "after": ["<", "g", "v"]
        },
        {
            "before": [">"],
            "after": [">", "g", "v"]
        },
    ],
  "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["x"],
            "after": ["\"", "_", "x"]
        },
        {
            "before": ["s"],
            "after": ["\"", "_", "s"]
        },
        {
            "before": ["u"],
            "commands": ["undo"]
        },
        {
            "before": ["<C-r>"],
            "commands": ["redo"]
        },
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": ["<C-u>"],
            "after": ["<Esc>", "v", "i", "w", "U", "e", "a"]
        },
    ]
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "notebook.markup.fontSize": 14,
}

